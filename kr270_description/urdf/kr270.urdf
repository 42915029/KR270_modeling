<!------------------------------------ Joints ----------------------------------->

	<joint name="joint_base" type="fixed">
		<parent link="world"/>
		<child link="socket"/>
	</joint>
	
	<joint name="joint_a1" type="revolute">
		<parent link="socket"/>
		<child link="a1"/>
		<origin rpy="0 0 0" xyz="0 0 0.25"/>
		<axis xyz="0 0 1"/>
		<limit effort="10000.0" lower="-3.2289" upper="3.2289" velocity="100.0"/>
	</joint>

	<joint name="joint_a2" type="revolute">
		<parent link="a1"/>
		<child link="a2"/>
		<origin rpy="0 0 0" xyz="-0.35 0.3 0.4"/>
		<axis xyz="0 1 0"/>
		<limit effort="10000.0" lower="-1.484" upper="0.873" velocity="100.0"/>
	</joint>

	<joint name="joint_a3" type="revolute">
		<parent link="a2"/>
		<child link="a3"/>
		<origin rpy="0 0 0" xyz="0 -0.3 1.15"/>
